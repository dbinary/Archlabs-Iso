# launch compton
al-compositor --start &

# restore wallpaper
nitrogen --restore &

# panel
al-tint2-session &
sleep 1; $HOME/.config/polybar/scripts/launch.sh &

# Openbox autostart
ob-autostart -d &

## GNOME PolicyKit authentication
/usr/lib/polkit-gnome/polkit-gnome-authentication-agent-1 &

# Required for xfce setting to work
xfsettingsd &

# Launch conky if session exists
sleep 2; al-conky-session &

xdg-user-dirs-gtk-update &

#al-hello

sleep 2; $HOME/.config/panel-switch &
