# launch compton
al-compositor --start &

# restore wallpaper
nitrogen --restore &

# panel
al-tint2-session &
$HOME/.config/polybar/scripts/launch.sh &

# Openbox autostart
ob-autostart -d &

# Required for xfce setting to work
xfsettingsd &

# Launch conky if session exists
sleep 2; al-conky-session &

xdg-user-dirs-gtk-update &

#al-hello
